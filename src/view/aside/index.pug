aside.side-menu
  s_sidebar
    <!-- 사이드 메뉴 -->
    s_sidebar_element
      <!-- 프로필 -->
      section#profile
        .img-wrapper
          img(src='[##_image_##]')
        .info
          .name [##_blogger_##]
          .desc [##_desc_##]

