  "scripts": {
    "build:pug": "yarn pug ./src/view/index.pug -o ./build/skin.html -P",
    "build": "yarn build:pug && yarn webpack --config webpack.config.js",
    "build:prod": "export NODE_ENV=prod && yarn build:pug && yarn webpack --config webpack.config.js",
